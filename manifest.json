{
  "name": "StopAD",
  "version": "1.0",
  "description": "Say GoodBye to Ads,Adblocker that doesn't get detected",
  "manifest_version": 2,

  "background": {
    "scripts": ["blockedsites_v2.js", "Uadblock.js"]
  },
  "icons": {
    "16": "icons/logo_16x16.png",
    "48": "icons/logo_48x48.png",
    "128": "icons/logo_128x128.png"
  },
  "permissions": ["webRequest", "webRequestBlocking", "http://*/", "https://*/"]
}
